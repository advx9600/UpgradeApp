<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_window_focused="false" android:state_enabled="true">
        <bitmap android:gravity="left" android:src="@drawable/yi_ic_titlebar_btn_left_normal" />
    </item>
    <item android:state_window_focused="false" android:state_enabled="false">
        <bitmap android:gravity="left" android:src="@drawable/yi_ic_titlebar_btn_left_disable" />
    </item>
    <item android:state_pressed="true">
        <bitmap android:gravity="left" android:src="@drawable/yi_ic_titlebar_btn_left_pressed" />
    </item>
    <item android:state_focused="true" android:state_enabled="true">
        <bitmap android:gravity="left" android:src="@drawable/yi_ic_titlebar_btn_left_normal" />
    </item>
    <item android:state_enabled="true">
        <bitmap android:gravity="left" android:src="@drawable/yi_ic_titlebar_btn_left_normal" />
    </item>
    <item android:state_focused="true">
        <bitmap android:gravity="left" android:src="@drawable/yi_ic_titlebar_btn_left_disable" />
    </item>
    <item>
        <bitmap android:gravity="left" android:src="@drawable/yi_ic_titlebar_btn_left_disable" />
    </item>
</selector>